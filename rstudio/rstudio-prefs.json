{
    "editor_theme": "Tomorrow Night Bright",
    "posix_terminal_shell": "zsh",
    "console_double_click_select": true,
    "save_workspace": "never",
    "default_project_location": "~/Documents/Code",
    "jobs_tab_visibility": "shown",
    "initial_working_directory": "~",
    "rainbow_parentheses": true,
    "git_exe_path": "/opt/homebrew/bin/git",
    "source_with_echo": true,
    "rmd_auto_date": true,
    "document_author": "pmhogan",
    "show_hidden_files": true,
    "load_workspace": false,
    "show_last_dot_value": true,
    "remove_history_duplicates": true,
    "auto_detect_indentation": true,
    "soft_wrap_r_files": true,
    "highlight_selected_line": true,
    "show_margin": false,
    "highlight_r_function_calls": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "auto_save_on_blur": true,
    "tab_multiline_completion": true,
    "code_completion_delay": 150,
    "warn_variable_defined_but_not_used": true,
    "style_diagnostics": true,
    "auto_discover_package_dependencies": false,
    "show_diagnostics_other": true,
    "fold_style": "begin-only",
    "syntax_color_console": true,
    "auto_expand_error_tracebacks": true,
    "font_size_points": 11,
    "save_files_before_build": true,
    "show_doc_outline_rmd": true,
    "doc_outline_show": "sections_and_chunks",
    "rmd_viewer_type": "pane",
    "latex_preview_on_cursor_idle": "never",
    "spelling_dictionary_language": "en_GB"
}